@import "../move-item-tpl/move-item-tpl.wxss";

.move-list {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  font-size: 28rpx;
}
.inner-container {
  padding: 0 20rpx;
}

.move-list .move-header {
  font-size: 32rpx;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
}
.move-list .move-header .more {
  float: right;
}

.move-list .move-content {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 376rpx;
}
.move-list .move-content .move-item-container:nth-child(3) {
  margin: 0;
}
